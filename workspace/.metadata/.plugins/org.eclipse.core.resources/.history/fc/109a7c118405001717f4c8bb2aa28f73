package programs;

public class Pattern {
	public static void main(String[] args) 
	{
		int rows=3;
		for(int i=1;i<=rows;i++)
		{
			for(int j=0;j<2*i-1;j++)
			{
				System.out.println(" ");
			}
			for(int k=0;k<2*i-1;k++)
			{
				System.out.print("* ");
			}
			System.out.println();
		}
		
	}
}
